{
  "name": "insaat-yonetim-backend",
  "version": "1.0.0",
  "description": "Insaat Yonetim Sistemi - Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:init": "psql -h $DB_HOST -U $DB_USER -d $DB_NAME -f database/schema.sql",
    "db:seed": "node seed-raw.js",
    "db:reset": "node resetDb-raw.js",
    "db:fresh": "node resetDb-raw.js && node seed-raw.js"
  },
  "keywords": [
    "construction",
    "management",
    "api",
    "postgresql"
  ],
  "author": "Dogukan",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}